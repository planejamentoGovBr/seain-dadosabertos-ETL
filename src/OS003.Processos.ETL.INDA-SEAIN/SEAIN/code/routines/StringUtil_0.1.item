package routines;

public class StringUtil {

    /**
     * padronizarString: not return value, only print "hello" + message.
     * 
     * 
     * {talendTypes} String
     * 
     * {Category} User Defined
     * 
     * {param} string("world") input: The string need to be printed.
     * 
     * {example} padronizarString("world") # hello world !.
     */
    public static String padronizarString(String string) {
    	return string.replaceAll("(\r\n|\n\r|\r|\n)", " ").replaceAll("\"", "") ;
    }
}
